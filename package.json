{
	"name": "rocket-blocks",
	"version": "0.1.0",
	"description": "Starter set up for rapidly developing native Gutenberg blocks",
	"author": "Anton Plauche",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"dev": "wp-scripts start",
		"env:start": "wp-env start",
		"env:destroy": "wp-env destroy",
		"env:stop": "wp-env stop",
		"add:block:basic": "cd src && npx @wordpress/create-block@latest --no-plugin --template ../templates/_basic-ssr --namespace rktblk --category text",
		"add:block:inner": "cd src && npx @wordpress/create-block@latest --no-plugin --template ../templates/_inner-blocks --namespace rktblk --category text"
	},
	"devDependencies": {
		"@wordpress/env": "^10.9.0",
		"@wordpress/scripts": "^30.1.0"
	},
	"dependencies": {
		"@wordpress/server-side-render": "^5.11.0"
	}
}
